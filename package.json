{
  "activationEvents": [
    "onStartupFinished",
    "onDebug",
    "workspaceContains:**/*.tdr"
  ],
  "categories": [
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "category": "Technical Debts",
        "command": "vstechdebt.addTechDebt",
        "title": "Add technical debt"
      }
    ],
    "languages": [
      {
        "extensions": [
          ".tdr"
        ],
        "id": "tdr"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "vscode-tdr.addTechDebt",
          "group": "techdebt",
          "title": "Add technical debt",
          "when": "explorerViewletVisible"
        }
      ]
    }
  },
  "description": "",
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3"
  },
  "displayName": "vscode-tdr",
  "engines": {
    "vscode": "^1.74.0"
  },
  "main": "./out/extension.js",
  "name": "vscode-tdr",
  "scripts": {
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -watch -p ./"
  },
  "version": "0.0.1"
}
